  color: rgba(63, 81, 181, 1);
  font-size: 48rpx;
  font-family: PingFangSC-bold;
  font-weight: bold;
}

.block_content
{
  width: 100%;

  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: flex-start;
}

