.outer
{
  width: 100%;
  height: 100%;

  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: flex-start;
}

.navigator
{
  width: 100%;

  z-index: 9999;
  position: absolute;

  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: flex-start;
}

.btn_nav_back
{
  width: 158rpx;
  height: 64rpx;
  margin-left: 26rpx;
  margin-top: 14rpx;

  background-color: #555454;
  border-radius: 6rpx;

  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}

.btn_nav_back_text
{
  color: rgba(255, 255, 255, 1);
  font-size: 32rpx;
  font-family: PingFangSC-bold;
  font-weight: bold;
}

.map_btn_group
{
  width: 100%;
  height: 60rpx;
  margin-top: 560rpx;

  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}

.btn_change_mapmode
{
  width: 60rpx;
  height: 60rpx;
  margin-left: 26rpx;
}

.btn_getlocation
{
  width: 60rpx;
  height: 60rpx;
  margin-right: 26rpx;
}

.block_map
{
  width: 100%;
  height: 660rpx;
}

.block_function
{
  width: 100%;

  position: absolute;

  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
}

.circle_bar
{
  width: 100%;
  height: 24rpx;

  border-radius: 20rpx 20rpx 0rpx 0rpx;
  background-color: white;
}

.card_tabBar
{
  width: 100%;
  height: 78rpx;
  white-space: nowrap;
  border-radius: 20rpx 20rpx 0rpx 0rpx;
  padding-left: 24rpx;
}

.card_slideBar
{
  width: 100%;

  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
}

.content_tab
{
  display: inline-block;
  margin-top: 4rpx;
  margin-right: 24rpx;
  width: 140rpx;
  height: 48rpx;
  border-radius: 38rpx;
  background-color: white;
  box-shadow: 0px 0px 2px 0px rgba(170, 170, 170, 1);
}

.content_tab_container
{
  width: 100%;
  height: 100%;

  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}

.content_tab_icon
{
  width: 28rpx;
  height: 28rpx;
}

.content_tab_text
{
  margin-left: 20rpx;
  color: #BBBBBB;
  font-size: 28rpx;
}

.content_tab_text_active
{
  margin-left: 20rpx;
  color: #76A6FB;
  font-size: 28rpx;
}

.card_bar
{
  width: 698rpx;
  height: 210rpx;
  border-radius: 20rpx;
  box-shadow: 2rpx 2rpx 4rpx 0rpx rgba(170, 170, 170, 1);
  margin-bottom: 38rpx;
  margin-top: 2rpx;
  margin-left: 20rpx;

  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: flex-start;
}

::-webkit-scrollbar
{
  display: none;
  width: 0;
  height: 0;
  color: transparent;
}

.card_bar_decorator
{
  width: 32rpx;
  height: 210rpx;
  margin-right: 70rpx;
  border-radius: 20rpx 0rpx 0rpx 20rpx;
}

.card_bar_cursor
{
  margin-left: 4rpx;
  margin-top: 16rpx;
  margin-right: 50rpx;
  width: 24px;
  height: 24px;
}

.card_bar_icon
{
  margin-top: 12rpx;
  width: 166rpx;
  height: 186rpx;

  background-color: yellow;
}

.card_bar_title_group
{
  margin-left: 38rpx;
  margin-top: 22rpx;
  width: 360rpx;
  height: 150rpx;

  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: flex-start;
}

.card_bar_title_group_title
{
  color: rgba(16, 16, 16, 0.93);
  font-size: 44rpx;
  font-family: SourceHanSansSC-regular;
}

.card_bar_title_group_line
{
  margin-top: 16rpx;
  width: 142rpx;
  height: 2rpx;
  border-top: 2rpx solid rgba(187, 187, 187, 1);
}

.card_bar_title_group_subtitle
{
  margin-top: 32rpx;
  color: rgba(159, 159, 159, 1);
  font-size: 24rpx;
  font-family: PingFangSC-regular;
}